---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>(emacs-nyc)</title>
  <link href="http://emacsnyc.org/atom.xml" rel="self"/>
  <link href="http://emacsnyc.org/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://emacsnyc.org/</id>
  <author>
    <name>(emacs-nyc)</name>
    <email>admin@emacsnyc.org</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <title type="html">{{ post.title | xml_escape }}</title>
    <link href="http://emacsnyc.org{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://emacsnyc.org{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
